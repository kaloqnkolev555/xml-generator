  "scripts": {
    "bundlesize": "bundlesize",
    "css": "npm-run-all --parallel css-lint* css-compile* --sequential css-prefix* css-minify*",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/bootstrap.scss css/bootstrap.css && node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/bootstrap-grid.scss css/bootstrap-grid.css && node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/bootstrap-reboot.scss css/bootstrap-reboot.css && node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/kpmg-web-light.scss css/kpmg-web-light.css && node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/kpmg-icons.scss css/kpmg-icons.css && node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/kpmg-logo.scss css/kpmg-logo.css",
    "css-lint": "stylelint --syntax scss \"scss/**/*.scss\"",
    "css-main": "npm-run-all --parallel css-lint css-compile --sequential css-prefix css-minify",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output css/bootstrap.min.css css/bootstrap.css && cleancss --level 1 --source-map --source-map-inline-sources --output css/bootstrap-grid.min.css css/bootstrap-grid.css && cleancss --level 1 --source-map --source-map-inline-sources --output css/bootstrap-reboot.min.css css/bootstrap-reboot.css && cleancss --level 1 --source-map --source-map-inline-sources --output css/kpmg-web-light.min.css css/kpmg-web-light.css && cleancss --level 1 --source-map --source-map-inline-sources --output css/kpmg-icons.min.css css/kpmg-icons.css && cleancss --level 1 --source-map --source-map-inline-sources --output css/kpmg-logo.min.css css/kpmg-logo.css",
    "css-nolint": "npm-run-all --parallel css-compile* --sequential css-prefix* css-minify*",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"css/*.css\" \"!css/*.min.css\"",
    "dist": "npm-run-all --parallel css js",
    "js": "npm-run-all js-lint* js-compile js-minify",
    "js-compile": "npm-run-all --parallel js-compile-*",
    "js-compile-bundle": "rollup --environment BUNDLE:true --config build/rollup.config.js --sourcemap",
    "js-compile-plugins": "cross-env PLUGINS=true babel js/src/ --out-dir js/dist/ --source-maps",
    "js-compile-standalone": "rollup --environment BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-lint": "eslint js/ build/",
    "js-main": "npm-run-all js-lint js-compile js-minify",
    "js-minify": "npm-run-all --parallel js-minify-*",
    "js-minify-bundle": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=js/dist/bootstrap.bundle.js.map,includeSources,url=bootstrap.bundle.min.js.map\" --output js/dist/bootstrap.bundle.min.js js/dist/bootstrap.bundle.js",
    "js-minify-standalone": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=js/dist/bootstrap.js.map,includeSources,url=bootstrap.min.js.map\" --output js/dist/bootstrap.min.js js/dist/bootstrap.js",
    "js-test": "npm-run-all --parallel js-test-karma*",
    "js-test-cloud": "ruby -r webrick -e \"s = WEBrick::HTTPServer.new(:Port => 3000, :DocumentRoot => Dir.pwd, :Logger => WEBrick::Log.new('/dev/null'), :AccessLog => []); trap('INT') { s.shutdown }; s.start\"",
    "js-test-karma": "karma start js/tests/karma.conf.js",
    "js-test-karma-old": "cross-env USE_OLD_JQUERY=true npm run js-test-karma",
    "maintenance-dependencies": "ncu -a -x jquery && npm update && bundle update && shx echo \"Manually update assets/js/vendor/*, js/tests/vendor/* and .travis.yml\"",
    "release-sri": "node build/generate-sri.js",
    "release-version": "node build/change-version.js",
    "release-zip": "cd dist/ && zip -r9 bootstrap-$npm_package_version-dist.zip * && shx mv bootstrap-$npm_package_version-dist.zip ..",
    "test": "npm-run-all dist js-test bundlesize",
    "watch": "npm-run-all --parallel watch-css watch-js",
    "watch-css": "nodemon --ignore js/ --ignore css/ -e scss -x \"npm run css\"",
    "watch-js": "nodemon --ignore scss/ --ignore js/dist/ --ignore css/ -e js -x \"npm run js-compile\""
  },
  "style": "css/bootstrap.css",
  "version": "4.1.3"
